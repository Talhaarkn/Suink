# SuiKnow Production Environment Variables
# Seal Protocol Configuration

# Sui Network Configuration
VITE_SUI_NETWORK=testnet
VITE_SUI_RPC_URL=https://fullnode.testnet.sui.io:443

# Package and Module Addresses
VITE_PACKAGE_ID=
VITE_QUIZ_MODULE=sui_kahoot

# Google OAuth Configuration for zkLogin
VITE_GOOGLE_CLIENT_ID=
VITE_GOOGLE_CLIENT_SECRET=
VITE_GOOGLE_REDIRECT_URI=http://localhost:3000/auth/callback

# Enoki API Configuration for Sponsored Transactions
VITE_ENOKI_PUBLIC_KEY=

# Seal Protocol Configuration (Testnet)
VITE_SEAL_KEY_SERVER_URL=http://localhost:2024
VITE_SEAL_PACKAGE_ID=

# Walrus Protocol Configuration
VITE_WALRUS_API_KEY=demo-api-key
VITE_WALRUS_API_URL=https://api.wal.app
VITE_WALRUS_CLI_PATH=./walrus.exe
VITE_WALRUS_CONFIG_PATH=~/.config/walrus/client_config.yaml

# Application Configuration
VITE_APP_NAME=SuiKnow
VITE_APP_DESCRIPTION=A decentralized quiz platform built on Sui blockchain
VITE_APP_URL=http://localhost:3000

# Production Settings
NODE_ENV=production
VITE_APP_ENV=production


